{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": [
                ".next/**",
                "!.next/cache/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "dev": {},
        "lint": {},
        "ci": {
            "dependsOn": [
                "^build",
                "^lint"
            ]
        }
    }
}