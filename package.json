    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "prettier": "^3.3.3",
    "turbo": "^1.10.16",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "dependencies": {
    "eslint-plugin-typescript-sort-keys": "^3.2.0"
